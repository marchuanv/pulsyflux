{
  "name": "pulsyflux-broker",
  "version": "1.0.0",
  "description": "Node.js bindings for PulsyFlux - high-performance pub/sub message broker with native C++ addon and Go shared library",
  "main": ".bin/release/registry.mjs",
  "type": "module",
  "exports": {
    ".": {
      "import": "./.bin/release/registry.mjs",
      "types": "./types/index.d.ts"
    }
  },
  "scripts": {},
  "files": [
    ".bin/release/",
    "README.md",
    "LICENSE",
    "types/"
  ],
  "devDependencies": {
    "jasmine": "^5.13.0",
    "node-addon-api": "^8.5.0",
    "node-api-headers": "^1.3.0",
    "zig-build": "github:solarwinds/zig-build",
    "tsx": "^4.21.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "os": [
    "win32"
  ],
  "cpu": [
    "x64"
  ],
  "keywords": [
    "pulsyflux",
    "pubsub",
    "pub-sub",
    "message-broker",
    "broker",
    "messaging",
    "events",
    "tcp",
    "multiplexing",
    "high-performance",
    "native-addon",
    "golang",
    "c++",
    "real-time"
  ],
  "author": {
    "name": "PulsyFlux Team"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/marchuanv/pulsyflux.git",
    "directory": "nodejs-api"
  },
  "homepage": "https://github.com/marchuanv/pulsyflux#readme",
  "bugs": {
    "url": "https://github.com/marchuanv/pulsyflux/issues"
  }
